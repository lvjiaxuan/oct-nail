{
  "name": "@lvjiaxuan/fastify-demo",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "description": "This project was bootstrapped with Fastify-CLI.",
  "author": "lvjiaxuan <471501748@qq.com> (https://github.com/lvjiaxuan)",
  "license": "MIT",
  "keywords": [],
  "main": "app.ts",
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "lint": "eslint .",
    "prisma:introspect ": "prisma db pull",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/sensible": "^5.5.0",
    "@prisma/client": "^5.4.2",
    "fastify": "^4.24.3",
    "fastify-cli": "^5.8.0",
    "fastify-plugin": "^4.5.1",
    "prisma": "^5.4.2"
  },
  "devDependencies": {
    "@lvjiaxuan/eslint-plugin": "^0.20.0",
    "@types/node": "^20.8.7",
    "@types/tap": "^15.0.10",
    "c8": "^8.0.1",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "tap": "^18.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "ts-node/esm"
    ],
    "coverage": false
  }
}
